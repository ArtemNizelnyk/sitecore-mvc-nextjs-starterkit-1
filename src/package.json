{
  "name": "@uniformdev/habitat-site-next",
  "version": "3.0.200729-3",
  "publishConfig": {
    "registry": "https://uniform.dev/no-matter-what-url/"
  },
  "private": true,
  "license": "SEE LICENSE IN PROVIDED DISTRIBUTION PACKAGE",
  "scripts": {
    "build": "next build",
    "dev": "cross-env UNIFORM_MODE=preview node ./server.js",
    "start": "next build && cross-env NODE_ENV=production node ./server.js",
    "format": "prettier --write \"{./components,./pages}/**/*.{js,ts,tsx}\"",
    "export:site": "next export --threads 1",
    "export": "npm-run-all --serial build export:site",
    "deploy": "node ./deploy.js",
    "bundle:theming": "npx uglify-js ./public/scripts/Sitecore.Foundation.Theming.js -o ./public/scripts/Sitecore.Foundation.Theming.min.js --compress --mangle --comments --source-map"
  },
  "dependencies": {
    "@uniformdev/next": "^3.0.200729-3",
    "@uniformdev/next-server": "^3.0.200729-3",
    "@uniformdev/publishing-all": "^3.0.200729-3",
    "dotenv": "^8.2.0",
    "next": "9.3.2",
    "next-compose-plugins": "^2.2.0",
    "react": "^16.13.1",
    "react-content-loader": "^5.0.2",
    "react-dom": "^16.13.1",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@uniformdev/publishing-awss3": "^3.0.200729-3",
    "@uniformdev/publishing-azureblobstorage": "^3.0.200729-3",
    "@uniformdev/publishing-netlify": "^3.0.200729-3",
    "cross-env": "^7.0.2",
    "mv": "^2.1.1",
    "npm-run-all": "^4.1.5"
  }
}
